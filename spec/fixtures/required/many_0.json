{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "logins",
    "passwords"
  ],
  "properties": {
    "logins": {
      "type": "object",
      "properties": {
        "login": {
          "type": "string"
        }
      }
    },
    "passwords": {
      "type": "object",
      "properties": {
        "password": {
          "type": "string"
        }
      }
    }
  }
}
